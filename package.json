  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "nextjs",
    "saas",
    "starter",
    "template",
    "authentication",
    "stripe",
    "payments",
    "drizzle",
    "typescript",
    "tailwind",
    "shadcn-ui",
    "team-management",
    "jwt",
    "postgresql"
  ],
