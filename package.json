{
  "name": "create-nextjs-mcp",
  "version": "1.0.0",
  "description": "MCP server that creates production-ready Next.js SaaS applications with authentication, payments, team management, and complete development setup",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "create-nextjs-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "global-install": "npm link",
    "global-uninstall": "npm unlink -g",
    "test": "tsx src/testing/run-tests.ts run",
    "test:cleanup": "tsx src/testing/run-tests.ts cleanup",
    "test:help": "tsx src/testing/run-tests.ts help"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
